	type location_unemployed_population_building_type = widget {
		layoutpolicy_vertical = fixed
		layoutpolicy_horizontal = fixed
		size = { 90 32 }

		hbox = {
			spacing = 5

			icon = {
				size = { 28 28 }
				layoutpolicy_vertical = fixed
				layoutpolicy_horizontal = fixed
				texture = "gfx/interface/icons/location_icons/new/population.dds"
			}

			text_single = {
				layoutpolicy_vertical = fixed
				layoutpolicy_horizontal = expanding
				text = "[InteractionTarget.GetLocation.GetUnemployedPopulationForBuildingTypeString(SelectInteractionTargetView.GetCurrentParams.GetTargetObjectFromFlag('building_type'))]"
			}
		}
	}
	
	type location_num_buildings_of_type = select_cell_default {
		size = { -1 32 }
		
		hbox = {
			spacing = 5
			text_single = {
				layoutpolicy_vertical = expanding
				layoutpolicy_horizontal = expanding
				text = "[InteractionTarget.GetLocation.GetNumBuildingLevelsOfBuildingTypeString(SelectInteractionTargetView.GetCurrentParams.GetTargetObjectFromFlag('building_type'),TargettedActionParameters.GetActor)]"
			}
		}
	}

	type location_control = select_cell_default {
		size = { -1 32 }
		hbox = {
			spacing = 5

			icon = {
				size = { 20 20 }
				texture = "gfx/interface/icons/location_icons/control.dds"

				glow = {
					name = "drop_shadow"
					glow_radius = 3
					color = {0.0 0.0 0.0 1.0}
					alpha = 0.3
				}
			}

			text_single = {
				layoutpolicy_vertical = expanding
				layoutpolicy_horizontal = expanding
				align = left|vcenter
				text = "[InteractionTarget.GetLocation.GetControl|0%]"
			}
		}

		datacontext = "[InteractionTarget.GetLocation]"
		tooltipwidget = {
			using = location_control_tooltip
		}

		sort_by_highlight = {
			size = { 100% 100% }
			name = "control"
		}
	}

