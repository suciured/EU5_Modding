types LocationColumns
{
	type location_name = select_cell_default {
		size = { -1 32 }
		blockoverride "interaction_target_action_provider_tooltip" {
			tooltipwidget = {
				using = action_tooltip_pop_out
				blockoverride "title_icon_texture" {
					texture = "[InteractionTarget.GetLocation.GetRankIcon]"
				}
				blockoverride "concept_link" {
					text = "[location|e]"
				}
			}
		}

		sort_by_highlight = {
			size = { 100% 100% }
			name = "name"
		}

		hbox = {
			margin = { 10 0 }
			spacing = 5

			icon = {
				size = { 18 18 }
				texture = "[InteractionTarget.GetLocation.GetRankIcon]"
			}

			text_single = {
				layoutpolicy_vertical = fixed
				layoutpolicy_horizontal = expanding
				autoresize = no
				align = nobaseline
				default_format = "#yellow_titles"
				text = "[InteractionTarget.GetLocation.GetNameWithNoTooltip]"
			}
		}
	}

	type location_population = select_cell_default {
		size = { 100 32 }

		sort_by_highlight = {
			name = "population"
		}
		
		datacontext = "[InteractionTarget.GetLocation]"
		tooltipwidget = {
			using = population_location_tooltip
		}

		hbox = {
			margin = { 5 0 }
			spacing = 10

			icon = {
				size = { 20 20 }
				layoutpolicy_vertical = fixed
				layoutpolicy_horizontal = fixed
				texture = "gfx/interface/icons/map_modes/population.dds"
				glow = {
					name = "drop_shadow"
					glow_radius = 3
					color = {0.0 0.0 0.0 1.0}
					alpha = 0.3
				}
			}

			text_single = {
				layoutpolicy_vertical = fixed
				layoutpolicy_horizontal = expanding
				align = nobaseline
				text = "[InteractionTarget.GetLocation.GetTotalPopulation]"
			}
		}
	}

	type location_owner_flag = widget {
		size = { 40 32 }
		# flag

		sort_by_highlight = {
			size = { 100% 100% }
			name = "owner_flag"
		}
		country_flag_small = {
			parentanchor = center
			layoutpolicy_vertical = fixed
			layoutpolicy_horizontal = fixed
			size = { 35 24 }
			datacontext = "[InteractionTarget.GetLocation.GetOwner]"
			blockoverride "hover_leave" {
				onmousehierarchyleave = "[SelectInteractionTargetGlue.OnMouseLeave(PdxGuiWidget.AccessSelf)]"
			}
		}
	}

	type army_construction_queue = widget {
		size = { 100 32 }

		sort_by_highlight = {
			size = { 100% 100% }
			name = "army_construction_queue"
		}

		hbox = {
			margin = { 5 0 }
			spacing = 10

			text_single = {
				layoutpolicy_vertical = fixed
				layoutpolicy_horizontal = expanding
				align = nobaseline
				text = "[GetDataModelSize(InteractionTarget.GetLocation.GetArmyConstructions)]"
				tooltipwidget = {
					SimpleContextualTooltipType = {
						blockoverride "title_icon" {
							icon = {
								using = tooltip_title_icon_size
								texture = "gfx/interface/icons/unit_category/mercenary.dds"
							}
						}

						blockoverride "concept_link" {
							text = "[mercenary|e]"
						}
																		
						textcontext = "arm_mercenary_construction_queue_simple"
							
					}
				}

			
				sort_by_highlight = {
					size = { 22% 150% }
					position = { -210 0 }
					name = "army_construction_queue"
				}
			}
		}
	}

