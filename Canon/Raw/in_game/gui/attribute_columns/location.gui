	type location_distance_to_target_area = select_cell_default {
		size = { -1 32 }
		hbox = {
			spacing = 5

			text_single = {
				margin_right = 10
				layoutpolicy_vertical = fixed
				layoutpolicy_horizontal = expanding
				autoresize = no
				align = right|nobaseline
				text = "[InteractionTarget.GetLocation.GetDistanceToTargetArea(SelectInteractionTargetView.GetCurrentParams.GetTargetObjectFromFlag('target').GetArea)|2]"
				tooltipwidget = {
						SimpleContextualTooltipType = {
							blockoverride "title_icon" {
								icon = {
									using = tooltip_title_icon_size
									texture = "gfx/interface/icons/outliner/categories/explorations.dds"
								}
							}

							blockoverride "concept_link" {
								text = "[exploration|e]"
							}
																			
							textcontext = "LOCATION_DISTANCE_TO_AREA"
								
						}
					}
				
				sort_by_highlight = {
					size = { 100% 100% }
					name = "distance_to_target_area"
				}
			}
		}
	}

	type build_roads = widget {
		allow_outside = yes
		size = { 480 35 }

		
		widget = {
			parentanchor = center
			size = { 460 32 }

			datacontext = "[InteractionTarget.GetLocation]"
			button = {
				size = { 100% 100% }
				using = select_interaction_target_action_provider

				hbox = {
					using = layoutpolicy_expanding

					widget = {

						size = { 30 30 }
						icon = {
							
							size = { 25 25 }
							texture = "gfx/interface/icons/roads/roads.dds"
							# texture = "[GetRoadTypeIcon(SelectInteractionTargetView.GetCurrentParams.GetTargetObjectFromFlag('road_type').GetRoadType)]"
							texture_density = 2
							parentanchor = center
						}
					}

					text_single = {
						margin_left = 5
						align = nobaseline
						using = layoutpolicy_expanding
						text = "[Location.GetName]"
					}
					text_single = {
						maximumsize = { 25 -1 }
						minimumsize = { 25 -1 }
						align = nobaseline
						text = "[GetRoadDistanceTo(Location.Self,SelectInteractionTargetView.GetCurrentParams.GetTargetObjectFromFlag('road_type').GetRoadType)]"
					}
					text_single = {
						maximumsize = { 150 -1 }
						minimumsize = { 150 -1 }
						using = layoutpolicy_expanding
						align = center|nobaseline
						text = "[Location.GetTopography.GetIcon][Location.GetTopography.GetNameNoUnderline]"
						default_format = "#subtle_name"
					}
					text_single = {
						margin_right = 15
						maximumsize = { 100 -1 }
						minimumsize = { 100 -1 }
						align = nobaseline
						text = "[GetRoadCostTo(Location.Self,SelectInteractionTargetView.GetCurrentParams.GetTargetObjectFromFlag('road_type').GetRoadType)]"
					}
				}
			}
		}
	}

