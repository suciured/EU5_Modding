template HolySites_Tooltip_Item {
	widget = {
		layoutpolicy_horizontal = expanding
		size = {-1 60}
		widget = {
			size = { 95% 50 }
			parentanchor = center
			
			tooltipwidget = {
				using = HolySite_tooltip
			}
			
			hbox = {
				using = layoutpolicy_expanding
				using = bg_scroll_card
				margin_left = 10
				
				bookmark_02 = {
					blockoverride "bookmark_size" {
						size = { 12 50 }
					}
					blockoverride "bookmark_texture" {
						texture = "gfx/interface/component_tiles/bookmark_02.dds"
