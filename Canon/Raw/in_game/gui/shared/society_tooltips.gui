template culture_tooltip_content {
	block "language_tooltip_top_content" {}

	TooltipListBase = {
		vbox = {
			layoutpolicy_horizontal = expanding
			ignoreinvisible = yes

			TooltipManualTableField = {
				spacing = 5

				icon = {
					size = { 25 25 }
					texture = "gfx/interface/icons/flat_icons/cultural_tradition.dds"
				}
				
				text_single = {
					layoutpolicy_horizontal = expanding
					text = "game_concept_cultural_tradition"
					default_format = "#explanation_link"
				}
				
				text_single = {
					raw_text = "[Culture.GetTradition|2Y]([Culture.GetTraditionChange|+=2])"
					align = right
				}

				tooltipwidget = {
					using = CulturalTradition_tooltip
				}
			}

			TooltipManualTableField = {
				spacing = 5

				icon = {
					size = { 25 25 }
					texture = "gfx/interface/icons/flat_icons/cultural_influence.dds"
				}
				
				text_single = {
					layoutpolicy_horizontal = expanding
					text = "game_concept_cultural_influence"
					default_format = "#explanation_link"
				}
				
				text_single = {
					raw_text = "[Culture.GetInfluence|2Y]([Culture.GetInfluenceChange|+=2])"
					align = right
				}

				tooltipwidget = {
					using = CulturalInfluence_tooltip
				}
			}
		}
	}

	TooltipStringPairList = {
		datacontext = "[StringToStringPairList(Culture.GetTooltip)]"
	}	
	
	TooltipTextBlock = {
		blockoverride "text" {
			datacontext = "[Culture]"
			text = "CULTURE_TOTAL_WORSHIPPERS"
		}
	}

	TooltipScrolledRowList = {
		blockoverride "block_scrollarea" { maximumsize = { -1 420 }}
		blockoverride "block_title" { text = "COUNTRIES_WITH_CULTURE" }
		blockoverride "rowlist_datamodel_is_empty" { visible = "[Not(DataModelHasItems(Culture.GetCountriesList.GetCountries))]"}
		blockoverride "rowlist_datamodel_has_items" { visible = "[DataModelHasItems(Culture.GetCountriesList.GetCountries)]"}
		blockoverride "rowlist_datamodel" { datamodel = "[Culture.GetCountriesList.GetCountries]" }
		blockoverride "empty_list_text" { text = "NO_COUNTRIES_WITH_CULTURE" }
		blockoverride "rowlist_content" {
			hbox = {
				using = layoutpolicy_expanding
				text_single = {
					visible = "[Not(Country.IsDominantCountryOfPrimaryCulture)]"

					layoutpolicy_horizontal = expanding
					autoresize = no
					text = "[Country.GetLongNameWithFlag]"
				}
				text_single = {
					visible = "[Country.IsDominantCountryOfPrimaryCulture]"

					layoutpolicy_horizontal = expanding
					autoresize = no
					text = "COUNTRY_WITH_DOMINANT"
				}

				text_single = {
					autoresize = yes
					raw_text = "[Culture.GetPopsForCountry(Country.Self)]@population!"
				}
			}
		}
	}

	block "culture_tooltip_player_status" {
		TooltipTextBlock = {	
			visible = "[Not(StringIsEmpty(Player.GetCultureStatus(Culture.Self)))]"
			blockoverride "text" {
				text = "[Player.GetCultureStatus(Culture.Self)]"
			}
		}	
	}
}

