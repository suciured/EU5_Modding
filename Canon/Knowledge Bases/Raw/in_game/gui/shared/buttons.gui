template button_regular_icon_color {
	using = color_new_gold
	modify_texture = {
		using = mask_scratches_texture
		alpha = 0.1
	}

	modify_texture = {
		using = overlay_stone_texture
		blend_mode = alphamultiply
		alpha = 0.2
	}
}

template targetted_action_button_regular {
	block "button_texture" {
		using = button_regular_texture
	}
	using = button_common_textobj_template
	block "tooltip" {
		tooltipwidget = { using = action_tooltip_pop_out }
	}
}

template targetted_action_button_default {
	size = { 120 30 }
	alpha = 0.9
	default_format = "#color_white"
	tooltipwidget = { using = action_tooltip_pop_out }

	block  "texture"
	{
		texture = "gfx/interface/buttons/button_default.dds"
		texture_density = 2
		spriteType = corneredtiled
		spriteborder = { 30 30 }
		framesize = { 126 40 }
	}
}

