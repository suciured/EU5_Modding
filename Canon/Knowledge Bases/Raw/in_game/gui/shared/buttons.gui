	type sort_by_key_button = button {
		block "sort_by_key_button_datacontext" {
			datacontext = "[FilteredSortedList.GetSortKeyButton(PdxGuiWidget.Self)]"
		}

		background = {
			visible = "[IsOdd_int32(PdxGuiWidget.GetIndexInParent)]"
			# block "bg_sort_highlight" {
				using = color_black_texture
			# }
			blend_mode = multiply
			alpha = 0.2
		}

		background = {
			visible = "[SortKey.IsSortedBy]"
			texture = "gfx/interface/component_masks/mask_fade_vertical_up.dds"
			texture_density = 2
			alpha = 0.4

			modify_texture = {
				using = color_sortby_off
				blend_mode = multiply
			}
		}

		down = "[SortKey.IsSortedBy]"

		action_tooltip = {
			click_type = left
			click_mode = single

			title = "[SortKey.GetTooltip]"
			on_action = "[SortKey.Sort]"
		}

		action_tooltip = {
			click_type = right
			click_mode = single
			title = "TOGGLE_ALL_LIST"
			visible = "[FilteredSortedList.ToggleAllEnabled]"
			on_action = "[FilteredSortedList.ToggleAll]"
		}

		tooltipwidget = {
			BasicFunctionalTooltip = {}
		}

		onmousehierarchyenter = "[SortKey.OnMouseHierarchyEnter]"
		onmousehierarchyleave = "[SortKey.OnMouseHierarchyLeave]"

		widget = {
			size = { 30 20 }
			parentanchor = center

			button_icon = {
				size = { 20 20 }

				visible = yes

				block "sort_by_key_button_texture" {
					texture = "[SortKey.GetIcon]"
				}
				texture_density = 2

				blockoverride "bg_button_color" {
					modify_texture = {
						visible = "[Not(SortKey.IsSortedBy)]"
						using = color_sortby_off
					}

					modify_texture = {
						visible = "[SortKey.IsSortedBy]"
						using = color_sortby_on
					}
				}

				action_tooltip = {
					click_type = left
					click_mode = single

					title = "[SortKey.GetTooltip]"
					on_action = "[SortKey.Sort]"
				}

				action_tooltip = {
					click_type = right
					click_mode = single
					title = "TOGGLE_ALL_LIST"
					visible = "[FilteredSortedList.ToggleAllEnabled]"
					on_action = "[FilteredSortedList.ToggleAll]"
				}

				tooltipwidget = {
					BasicFunctionalTooltip = {}
				}

				down = "[SortKey.IsSortedBy]"

			}

			icon = {
				size = { 8 8 }
				position = { 22 0 }
				name = "sort ascending"
				visible = "[Or(Not(SortKey.IsSortedBy), Not(SortKey.IsSortedInDescending))]"

				texture = "gfx/interface/icons/flat_icons/arrow_sortby.dds"
				texture_density = 2

				modify_texture = {
					using = color_sortby_on
					alpha = "[TransparentIfFalse(And(SortKey.IsSortedBy, Not(SortKey.IsSortedInDescending)))]"
				}

				glow = {
					visible = "[SortKey.IsSortedBy]"
					glow_radius = 3
					color = { 1.0 0.85 0.55 1.0 }
					alpha = 0.6
				}
			}

			icon = {
				size = { 8 8 }
				position = { 22 12 }
				name = "sort descending"
				visible = "[Or(Not(SortKey.IsSortedBy), SortKey.IsSortedInDescending)]"

				texture = "gfx/interface/icons/flat_icons/arrow_sortby.dds"
				texture_density = 2
				mirror = vertical

				modify_texture = {
					using = color_sortby_on
					alpha = "[TransparentIfFalse(And(SortKey.IsSortedBy, SortKey.IsSortedInDescending))]"
				}

				glow = {
					visible = "[SortKey.IsSortedBy]"
					glow_radius = 3
					color = { 1.0 0.85 0.55 1.0 }
					alpha = 0.6
				}
			}
		}
