	type header_action_button_left = action_button {
		using = bg_navigational_header_button_left
		using = header_button_template
		tooltipwidget = { using = action_tooltip_pop_out }

		blockoverride "header_button_orientation" {
			block "header_button_orientation" {
				righttoleft = no
			}
		}
		blockoverride "header_button_text_margins" {
			block "header_button_text_margins" {
				margin_left = 5
				margin_right = 20
			}
		}
		blockoverride "header_button_text_align" {
			block "header_button_text_align" {
				align = left|nobaseline
			}
		}
	}

