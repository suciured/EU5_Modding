	type button_back_alt = ui_direction_button_alt {
		block "button_texture" {
			using = back_button_alt_texture
		}

		background = {
			using = bg_button_light_round_texture
			modify_texture = {
				using = color_dark_blue_texture
			}
		}

		block "back_properties" {
			parentanchor = vcenter|left
		}

		block "back_onclick" {}

		tooltipwidget = {
			LeftRightClickButtonTooltip = {
				blockoverride "onclick_text" { text = "BACK" }
				blockoverride "onrightclick_text" { text = "SHOW_HISTORY" }
			}
		}

		using = snd_UI_backward
	}

	type button_back = ui_direction_button {
		block "button_texture" {
			texture = "gfx/interface/buttons/flats/back_button.dds"
			texture_density = 2
		}
		block "back_properties" {
			layoutpolicy_horizontal = expanding
			parentanchor = left|top
			position = { -5 -6 }
			size = { 23 23 }
		}

		block "back_onclick" {}
		tooltipwidget = {
			LeftRightClickButtonTooltip = {
				blockoverride "onclick_text" { text = "BACK" }
				blockoverride "onrightclick_text" { text = "SHOW_HISTORY" }
			}
		}
		using = bg_circle
		using = snd_UI_backward
	}

