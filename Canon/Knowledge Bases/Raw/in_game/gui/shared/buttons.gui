template action_button_common_template {
	visible = "[UIAction.IsVisible]"
	enabled = "[UIAction.IsEnabled]"
	onpressed = "[UIAction.OnLeftMouseDown]"
	onreleased = "[UIAction.OnLeftMouseUp]"
	onrightpressed = "[UIAction.OnRightMouseDown]"
	onrightreleased = "[UIAction.OnRightMouseUp]"
	onclick = "[UIAction.OnLeftClick]"
	onrightclick = "[UIAction.OnRightClick]"
	tooltipwidget = { using = action_tooltip_pop_out }
	using = snd_UI_open
	using = snd_UI_buttonPress
}

template ui_direction_targetted_action_button {
	# Properties to reproduce visuals from ui_direction_button, also added from button_icon and button
	shaderfile = "gfx/FX/pdxgui_pushbutton.shader"

	#####

	# block "action_button_modify_texture" {
		using = Master_Button_Modify_Texture
	# }
	# block "button_frames" {
		gfxtype = togglepushbuttongfx
		effectname = "NoHighlight"

		upframe = 1
		uphoverframe = 2
		uppressedframe = 3
		downframe = 2
		downhoverframe = 2
		downpressedframe = 3
		disableframe = 4
	# }

	#####

	block "texture" {
		texture = "gfx/interface/icons/flat_icons/close_flat_icon.dds"
	}
	texture_density = 2
	size = { 22 22 }
	modify_texture = {
		using = overlay_window_texture
		blend_mode = overlay
		alpha = 0.7
	}

	glow = {
		glow_radius = 5
		color = { 1.0 0.85 0.55 1.0 }
		alpha = 0.6

		block "glow_visible" {
			visible = no
		}
	}

	#####

	using = action_button_common_template
}

template button_open_template {
	texture = "gfx/interface/buttons/checkbox_open.dds"
	texture_density = 2
	frame_grid = { 2 1 }
	size = { 30 30 }

	background = {
		texture = "gfx/interface/buttons/checkbox_bg.dds"
		texture_density = 2
		using = color_dark_blue
	}
}

template button_subsidized_template {
	texture = "gfx/interface/buttons/checkbox_subsidized.dds"
	texture_density = 2
	frame_grid = { 2 1 }
	size = { 30 30 }

	background = {
		texture = "gfx/interface/buttons/checkbox_bg.dds"
		texture_density = 2
		using = color_dark_blue
	}
}

template button_visibility_template {
	texture = "gfx/interface/buttons/checkbox_visibility.dds"
	texture_density = 2
	frame_grid = { 2 1 }
	size = { 30 30 }

	blockoverride "master_color_frame" {
		frame = 1
	}
}

