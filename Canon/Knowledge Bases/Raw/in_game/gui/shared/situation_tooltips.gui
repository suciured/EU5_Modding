template Situation_tooltip {
	ContextualTooltipType = {
		blockoverride "title_text" {
			text = "[Situation.GetNameWithNoTooltip]"
		}
		
		blockoverride "title_icon_texture" {
			texture = "[Situation.GetIcon]"
		}

		blockoverride "title_button" {
			hint_button = {
				visible = "[Situation.HasHint]"
				action_tooltip = {
					title = "OPEN_HINT"
					on_action = "[OpenLateralViewWithParams('hints', LinkParam('selected_hint', Situation.GetHintTag))]"
				}
			}
		}

		blockoverride "concept_link" {
			text = [situation|E]
		}

		blockoverride "tooltip_content" {
			TooltipFlavorTextBlock = {
				blockoverride "text" {
					text = "[Situation.GetTooltip]"
				}
			}
		}
	}
}

template Situation_WarOfReligions_tooltip {
	ContextualTooltipType = {
		blockoverride "title_text" {
			text = "[Situation.GetNameWithNoTooltip]"
		}
		
		blockoverride "title_icon_texture" {
			texture = "[Situation.GetIcon]"
		}

		blockoverride "concept_link" {
			text = [situation|E]
		}

		blockoverride "tooltip_content" {
			TooltipScrolledContentSection = {
				blockoverride "section_content" {
					TooltipTextBlock = {
						minimumsize = { 420 0 }
						set_parent_size_to_minimum = yes
						visible = "[Not( StringIsEmpty( GetUniqueInternationalOrganization('hre').GetMonthlyEffectText ) )]"
						blockoverride "text" {
							text = "[GetUniqueInternationalOrganization('hre').GetMonthlyEffectText]"
						}
					}
					TooltipRowList = {
						minimumsize = { 420 0 }
						set_parent_size_to_minimum = yes
						visible = "[Not(IsDataModelEmpty(GetUniqueInternationalOrganization('hre').GetPolicies))]"
						blockoverride "block_title" { text = "INTERNATIONAL_ORGANIZATION_TT_LAWS_INFO" }
						blockoverride "rowlist_datamodel_is_empty" { visible = "[IsDataModelEmpty(GetUniqueInternationalOrganization('hre').GetPolicies)]"}
						blockoverride "rowlist_datamodel_has_items" { visible = "[DataModelHasItems(GetUniqueInternationalOrganization('hre').GetPolicies)]"}
						blockoverride "rowlist_datamodel" { datamodel = "[GetUniqueInternationalOrganization('hre').GetPolicies]" }
						blockoverride "rowlist_item" { text = "[Policy.GetName]" }
					}
					TooltipRowList = {
						minimumsize = { 420 0 }
						set_parent_size_to_minimum = yes
						visible = "[Not(IsDataModelEmpty(GetUniqueInternationalOrganization('hre').GetPayments))]"
						blockoverride "block_title" { text = "INTERNATIONAL_ORGANIZATION_TT_PAYMENTS_INFO" }
						blockoverride "rowlist_datamodel_is_empty" { visible = "[IsDataModelEmpty(GetUniqueInternationalOrganization('hre').GetPayments)]"}
						blockoverride "rowlist_datamodel_has_items" { visible = "[DataModelHasItems(GetUniqueInternationalOrganization('hre').GetPayments)]"}
						blockoverride "rowlist_datamodel" { datamodel = "[GetUniqueInternationalOrganization('hre').GetPayments]" }
						blockoverride "rowlist_item" { text = "[Payment.GetNameWithContext(GetUniqueInternationalOrganization('hre').Self)]" }
					}
					TooltipTextBlock = {
						minimumsize = { 420 0 }
						set_parent_size_to_minimum = yes
						visible = "[Not( StringIsEmpty( GetUniqueInternationalOrganization('hre').GetJoinWarsText ) )]"
						blockoverride "text" {
							text = "[GetUniqueInternationalOrganization('hre').GetJoinWarsText]"
						}
					}
					TooltipStringPairList = {
						minimumsize = { 420 0 }
						set_parent_size_to_minimum = yes
						visible = "[Not( StringIsEmpty( GetUniqueInternationalOrganization('hre').GetInternationalOrganizationEffects ) )]"
						textcontext = "[GetUniqueInternationalOrganization('hre').GetInternationalOrganizationEffects]"
					}
					TooltipStringPairList = {
						minimumsize = { 420 0 }
						set_parent_size_to_minimum = yes
						visible = "[Not( StringIsEmpty( GetUniqueInternationalOrganization('hre').GetLeaderEffects ) )]"
						textcontext = "[GetUniqueInternationalOrganization('hre').GetLeaderEffects]"
					}
					TooltipStringPairList = {
						minimumsize = { 420 0 }
						set_parent_size_to_minimum = yes
						visible = "[Not( StringIsEmpty( GetUniqueInternationalOrganization('hre').GetAllMembersEffects ) )]"
						textcontext = "[GetUniqueInternationalOrganization('hre').GetAllMembersEffects]"
					}
					TooltipRowList = {
						set_parent_size_to_minimum = yes
						blockoverride "block_title" { raw_text = "[GetUniqueInternationalOrganization('hre').GetMembersAmount] [Localize('INTERNATIONAL_ORGANIZATION_TT_INFO')]" }
						blockoverride "rowlist_datamodel_is_empty" { visible = "[IsDataModelEmpty(GetUniqueInternationalOrganization('hre').GetMembers)]"}
						blockoverride "rowlist_datamodel_has_items" { visible = "[DataModelHasItems(GetUniqueInternationalOrganization('hre').GetMembers)]"}
						blockoverride "rowlist_datamodel" { datamodel = "[DataModelFirst(GetUniqueInternationalOrganization('hre').GetMembers,'(int32)4')]" }
						blockoverride "rowlist_item" { text = "[Country.GetLongName]" }
						TooltipTableField = {
							visible = "[GreaterThan_int32(GetDataModelSize(GetUniqueInternationalOrganization('hre').GetMembers), '(int32)4')]"
							blockoverride "field_content" {
								textbox = {
									margin = { 10 5 }
									align = nobaseline
									autoresize = yes
									using = text_multi_template
									elide = right
									using = tooltip_max_width_title_wide
									text = "AND_SOME_MORE_IO"
								}
		
								expand = {}
							}
							blockoverride "field_background" {}
						}
					}
				}
			}
			TooltipFlavorTextBlock = {
				blockoverride "text" {
					text = "[Localize( Concatenate(GetUniqueInternationalOrganization('hre').GetType.GetNameKey, '_desc') )]"
				}
			}
		}
	}
}

