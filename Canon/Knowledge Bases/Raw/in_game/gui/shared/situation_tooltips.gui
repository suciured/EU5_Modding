template CardinalInSchism_tooltip {
	ContextualTooltipType = {
		blockoverride "title_text" {
			text = "SCHISM_CARDINALS_OF_COUNTRY"
		}

		blockoverride "title_icon" {
			country_flag_small = {}
		}

		blockoverride "concept_link" {
			text = "[cardinal|e]"
		}
		blockoverride "description_text" {
			text = ""
		}
		blockoverride "tooltip_content" {
			TooltipScrolledRowList = {
				blockoverride "block_scrollarea" { maximumsize = { -1 420 }}
				blockoverride "block_title" { visible = no }
				blockoverride "rowlist_datamodel_is_empty" { visible = "[Not(GreaterThan_int32(GetDataModelSize(Country.GetCardinals), '(int32)0'))]"}
				blockoverride "rowlist_datamodel_has_items" { visible = "[GreaterThan_int32(GetDataModelSize(Country.GetCardinals), '(int32)0')]"}
				blockoverride "empty_list_text" { text = "NO_CARDINALS_IN_COUNTRY" }
				blockoverride "rowlist_datamodel" { datamodel = "[Country.GetCardinals]" }

				blockoverride "rowlist_item" {
					datacontext = "[Cardinal]"
					text = "SCHISM_CARDINAL_ENTRY"
				}
			}
		}
	}
}




