template Annexation_tooltip {
	ContextualTooltipType = {
		blockoverride "title_text" {
			text = "DIP_ANNEXATION_ONGOING"
		}
		
		blockoverride "concept_link" {
			text = [annexing|e]
		}

		blockoverride "title_icon" {
			icon = {
				using = tooltip_title_icon_size
				texture = "gfx/interface/icons/outliner/categories/annexation.dds"
			}
		}

		
		blockoverride "tooltip_content" {
			hbox = {
				vbox = {
					widget = {
						size = { 120 120 }
						# using = layoutpolicy_expanding
						using = marker_bg_circle

						piechart = {
							parentanchor = center
							block "piechart_size" {
								size = { 90% 90% }
							}
							using = piechart_angles
							visible = "[Not(Country.GetDiplomacy.IsBeingAnnexedStalled)]"

							pieslice = {
								texture = "gfx/interface/pie_charts/pie_chart_alpha_80.dds"
								value = "[Country.GetDiplomacy.GetBeingAnnexedProgress]"
								color = { 0.01 0.6 0.9 1 }
								alpha = 1
							}

							pieslice = {
								texture = "gfx/interface/pie_charts/pie_chart_alpha_80.dds"
								value = "[Subtract_float('(float)100.0', Country.GetDiplomacy.GetBeingAnnexedProgress)]"
								color = { 0.0 0.1 0.1 1 }
								alpha = 1
							}
						}

						piechart = {
							parentanchor = center
							block "piechart_size" {
								size = { 90% 90% }
							}
							using = piechart_angles
							visible = "[Country.GetDiplomacy.IsBeingAnnexedStalled]"

							pieslice = {
								texture = "gfx/interface/pie_charts/pie_chart_alpha_80.dds"
								value = "[Country.GetDiplomacy.GetBeingAnnexedProgress]"
								color = { 0.5 0.5 0.5 1 }
								alpha = 1
							}

							pieslice = {
								texture = "gfx/interface/pie_charts/pie_chart_alpha_80.dds"
								value = "[Subtract_float('(float)100.0', Country.GetDiplomacy.GetBeingAnnexedProgress)]"
								color = { 0.0 0.1 0.1 1 }
								alpha = 1
							}
						}

						icon = {
							size = { 55% 55% }
							parentanchor = vcenter|hcenter
							tooltip_enabled = no
							texture = "gfx/interface/icons/outliner/categories/annexation.dds"
							texture_density = 2
						}
					}

					text_single = {
						text = "[Country.GetDiplomacy.GetBeingAnnexedDate]"						
						default_format = "#subtle_name"
					}
				}
				widget = {
					size = { 30 30 }
				}
				
				TooltipContentSection = {
					TooltipStringPairList = {
						textcontext =  "[Country.GetDiplomacy.GetBeingAnnexedInfo]"
					}
					TooltipStringPairList = {
						textcontext =  "[Country.GetDiplomacy.GetBeingAnnexedCostInfo]"
					}
				}
			}
		}
	}
}

template CountryTooltip_RelativePower {
	TooltipListBase = {
		visible = "[And3(Not(IsLobbyOpen), IsPlayerValid, Not(Country.IsPlayer))]"
		hbox = {
			layoutpolicy_horizontal = expanding
			margin = { 10 3 }
			spacing = 5
			tooltipwidget = { using = RelativePower_tooltip_country }
			
			widget = {
				size = { 25 25 }
				tagtooltip_enabled = yes

				icon = {
					size = { 100% 100% }
					texture = "gfx/interface/icons/foreign_country/country_power.dds"
					texture_density = 2
					alpha = 1
					glow = {
						name = "drop_shadow"
						glow_radius = 3
						color = {0.0 0.0 0.0 1.0}
						alpha = 0.5
					}
				}
			}
			
			text_multi = {
				layoutpolicy_horizontal = expanding
				size = {-1 25}
				autoresize = no
				align = left|vcenter
				default_format = "#yellow_titles"
				text = "[GetRelativePowerDescriptionTooltip(Player.Self, Country.Self)]"
				fontsize = 14
				using = FontDefaultColor
			}
		}
	}
}

